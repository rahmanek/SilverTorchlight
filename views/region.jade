script(type="text/ng-template" id="region.html")
	div(id="regionDescription")
		h2 {{region.description.longName}}
		p Regional Profile
	div(id="subnav" style="width:960px; margin: 30px 0 30px 0;")
		ul.nav.nav-tabs
			li(ng-click = "regionTab = 'regionTme'")
				a(href='javascript:;') TME
			li(ng-click = "regionTab = 'regionPayerMix'")
				a(href='javascript:;') Payer Mix
			li(ng-click = "regionTab = 'regionOther'")
				a(href='javascript:;') Other

	div(ng-show="regionTab == 'regionTme'" style="position:relative;")
		div(id="tmeDonut" style="width 460px;")
		div(id="tmeDonutInfo" style="width:500px; height: 200px;position:absolute; right:0; top:0;border-left: 3px solid #000000; padding-left: 15px;")
			span(id="tmeDonutInfoCat" style="font-size:24px;") {{region.donut.category}}
			p(style="margin-top:20px;") Medical Expense PMM: ${{region.donut.value}}
			p(style="margin-top:20px;") Percent of Total PMM: {{region.donut.percent}} %
	div(ng-show="regionTab == 'regionPayerMix'")
		p stuff 2
	div(ng-show="regionTab == 'regionOther'")
		p stuff 3